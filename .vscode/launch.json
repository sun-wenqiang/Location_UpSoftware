{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Qt Program",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/myproject",  // 编译生成的可执行文件路径
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "make",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
